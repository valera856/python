# ex2
a = 8 
b = 10
sleep = int(input("Сколько спит Аня:"))
if sleep >= a and sleep < b:
    print("Это нормально")
elif sleep < a:
    print("Недосып")
elif sleep > b:
    print("Пересып")
# ex3
a = int(input("Введите число"))
b = int(input("Введите число"))
c = int(input("Введите число"))
s = (a + b + c)/2
x=(s*(s - a)*(s - b)*(s - c))** 0.5
print(x)
# ex4
print("Выберите тип комнаты : \п[1] - Треугольная \п[2] - Прямоугольная \п[3] - Круглая ")
num = input ("Введите номер вышего выбора: ")
if num == "1":
    a = int (input ("Введите длину стены а: "))
    b = int (input ("Введите длину стены b: "))
    c = int (input ("Введите длину стены с: "))
    p= (a + b+ c) / 2
    s = math.sqrt(p*(p - a)*(p-b)*(p-c))
    print ("Площадь комнаты будет равна: ", s)
elif num == "2":
    a = int(input("Введите длину стены a: "))
    b = int (input("Введите длину стены b: "))
    s= a * b
    print ("Площадь комнаты будет равна: ", s)
elif num == "3":
    d = int (input("Введите шиирину комнаты: "))
    r= d/2
    pi = 3.14
    s= pi * (r**2)
    print("Площадь комнаты будет равна: ", s)
else:
    print ("Неверная операция!")
# ex5
n = int(input("Введиче число"))
if n==1:
    print(n, "Программист")
elif n>=2 and n<=4:
    print(n, "Программиста")
elif n>=5:
    print(n, "Программистов")
